{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/casestudies-related-posts",
  "version": "0.1.0",
  "title": "Casestudies Related Posts",
  "category": "qrolic",
  "icon": "layout",
  "description": "This block is used to display the post related to the case study, which includes the title, description and post link.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "casestudies-related-posts",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "attributes": {
    "numberOfItems": {
      "type": "number",
      "default": 2
    },
    "columns": {
      "type": "number",
      "default": 1
    },
    "displayDate": {
      "type": "boolean",
      "default": true
    },
    "displayExcerpt": {
      "type": "boolean",
      "default": true
    },
    "displayThumbnail": {
      "type": "boolean",
      "default": true
    },
    "readMore": {
      "type": "string",
      "source": "html"
    },
    "Title": {
      "type": "string",
      "default": ""
    }
  }
}