{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/blog-recommended-articles-v2",
  "version": "0.1.0",
  "title": "Blog Recommended Articles V2",
  "category": "qrolic",
  "icon": "list-view",
  "description": "A dynamic block to display recommended blog articles within a some category of blog posts.",
  "example": {},
  "attributes": {
    "numberOfItems": {
      "type": "number",
      "default": 2
    },
    "columns": {
      "type": "number",
      "default": 1
    },
    "displayDate": {
      "type": "boolean",
      "default": false
    },
    "displayExcerpt": {
      "type": "boolean",
      "default": true
    },
    "displayThumbnail": {
      "type": "boolean",
      "default": true
    },
    "showBio": {
      "type": "boolean",
      "default": true
    },
    "Title": {
      "type": "string",
      "default": ""
    },
    "selectedPostID": {
      "type": "string",
      "default": ""
    },
    "SectionPadding": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false
  },
  "textdomain": "blog-recommended-articles-v2",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}