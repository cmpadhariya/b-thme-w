{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/career-related-posts",
  "version": "0.1.0",
  "title": "Career Related Posts",
  "category": "qrolic",
  "icon": "  dashicons-chart-bar",
  "description": "This block stores the related post of the career custom post type in the career single page.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "career-related-posts",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "attributes": {
    "numberOfItems": {
      "type": "number",
      "default": 5
    },
    "Heading": {
      "type": "string",
      "default": "Similar Openings"
    },
    "category": {
      "type": "string",
      "default": "fulltime"
    },
    "location": {
      "type": "string",
      "default": "Rajkot, Gujarat"
    },
    "buttonText": {
      "type": "string",
      "default": " View Posting"
    }
  }
}