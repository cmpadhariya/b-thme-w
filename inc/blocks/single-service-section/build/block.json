{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/single-service-section",
  "version": "0.1.0",
  "title": "Single Service Section",
  "category": "qrolic",
  "icon": "category",
  "description": "This block is a Single Service Section block, with user imform for qrolic services.",
  "supports": {
    "html": false
  },
  "attributes": {
    "Heading": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    },
    "Content01": {
      "type": "string",
      "source": "html",
      "selector": "p"
    },
    "mediaID": {
      "type": "number",
      "default": null
    },
    "mediaURL": {
      "type": "string",
      "default": null
    },
    "iconClass": {
      "type": "string",
      "default": ""
    },
    "altText": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "single-service-section",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}