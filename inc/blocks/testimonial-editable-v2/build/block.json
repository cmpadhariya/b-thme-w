{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/testimonial-editable-v2",
  "version": "0.1.0",
  "title": "Testimonial Editable V2",
  "category": "qrolic",
  "icon": "dashicons-format-image",
  "description": "This block is a Single testimonial image editable block, It's a simple section that highlights the editable testimonial.",
  "example": {},
  "supports": {
    "html": false,
    "className": true
  },
  "attributes": {
    "positiondescription": {
      "type": "string",
      "selector": "p",
      "default": ""
    },
    "imageId": {
      "type": "number",
      "default": 0
    },
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "quoteImage": {
      "type": "object",
      "default": {
        "id": null,
        "url": "",
        "alt": ""
      }
    },
    "heading": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "position": {
      "type": "string",
      "default": ""
    },
    "imgWidth": {
      "type": "string",
      "default": ""
    },
    "imgHeight": {
      "type": "string",
      "default": ""
    },
    "mediaID": {
      "type": "number",
      "default": null
    },
    "mediaURL": {
      "type": "string",
      "default": null
    },
    "altText": {
      "type": "string",
      "default": ""
    },
    "className": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "testimonial-editable-v2",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}