{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/our-solution-section-v2",
	"version": "0.1.0",
	"title": "Our Solution Section V2",
	"category": "qrolic",
	"icon": "  dashicons-admin-tools",
	"description": "This block shows the process of our solution of website include image, title and description.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "our-solution-section-v2",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"sectiontitle": {
            "type": "string",
            "default": "title"
        },
		"sectiontitlealignment": {
            "type": "string",
            "default": "left"
        },
		"number": {
            "type": "string",
            "default": "00"
        },
		"heading": {
            "type": "string",
            "default": "Heading"
        },
		"description":{
            "type": "string",
            "default": "Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua."
        },
		"items": {
            "type": "array",
            "default": []
        }
	}
}
